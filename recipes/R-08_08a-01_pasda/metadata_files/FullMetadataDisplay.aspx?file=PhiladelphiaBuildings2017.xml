    <DL>
      <DT>
        <I>Identification_Information:</I>
      </DT>
      <DD>
        <DL>
          <DT>
            <I>Citation:</I>
          </DT>
          <DD>
            <DL>
              <DT>
                <I>Citation_Information:</I>
              </DT>
              <DD>
                <DL>
                  <DT>
                    <I>Originator:</I>City of Philadelphia</DT>
                  <DT>
                    <I>Publication_Date:</I>2017</DT>
                  <DT>
                    <I>Title:</I>
                  </DT>
                  <DD>Philadelphia Buildings 2017</DD>
                  <DT>
                    <I>Geospatial_Data_Presentation_Form:</I>vector digital data</DT>
                  <DT>
                    <I>Online_Linkage:</I>
                    <A TARGET="viewer" HREF="http://www.pasda.psu.edu">http://www.pasda.psu.edu</A>
                  </DT>
